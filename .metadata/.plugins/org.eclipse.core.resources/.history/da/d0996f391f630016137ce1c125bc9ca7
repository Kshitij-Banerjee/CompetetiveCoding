/**
 * Definition for singly-linked list.
 */

  struct ListNode {
      int val;
      ListNode *next;
      ListNode(int x) : val(x), next(0) {}
  };
#define NULL 0;
class Solution {

public:
    ListNode* addTwoNumbers(ListNode* l1, ListNode* l2) {
    	ListNode* head ;

    	while(l1 != NULL || l2 != NULL){
    		ListNode* sum = new ListNode(l1->val + l2->val);
    	}

    	return head;
    }
};
