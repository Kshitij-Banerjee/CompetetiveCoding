#include <iostream>
#include <vector>

using namespace std;

typedef vector<int>::iterator vi;
typedef vector<int> v;

class Solution {
public:
    int maxArea(vector<int>& height) {

    	int max_area = 0;
    	for(vi i : height){
    		for(vi j = i + 1; j != height.end(); j++) {
    			max_area = max(max_area, min(*j , *i) * (j-i));
    		}
    	}

    	return max_area;
    }
};


int main() {

	Solution s;
	cout << s.maxArea(new vector<int>())
	return 0;
}
